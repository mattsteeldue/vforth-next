// +012
S0_origin:      dw      S0_system
R0_origin:      dw      R0_system
TIB_origin:     dw      TIB_system
WIDTH_origin:   dw      31
WARNING_origin: dw      1
FENCE_origin:   dw      Fence_Word
DP_origin       dw      Fence_Word
VOCLINK_origin: dw      Voclink_Ptr
                dw      FIRST_system
